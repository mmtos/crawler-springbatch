<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Hospital">
    <select id="selectAll" resultType="Hospital">
        SELECT RNUM,DUTY_NAME FROM HOSPITAL
    </select>
    <update id="mergeHospital" parameterType="Hospital">
        INSERT INTO HOSPITAL( RNUM, DUTY_NAME)
        VALUES (#{rnum},#{dutyName})
        ON DUPLICATE KEY UPDATE DUTY_NAME = #{dutyName}
    </update>
</mapper>